@import './App.styl'

 .container
    display grid
    grid-template-columns repeat(3, 1fr)
    padding 25px
    gap 20px
    margin 20px
    background-color #F9FAFC


@media screen and (max-width 800px)
  .container
    grid-template-columns repeat(2, 1fr)
   

@media screen and (max-width 575px)
  .container
    grid-template-columns 1fr
